#Empeche l'acces aux dossiers 
order allow,deny
allow from all
options -indexes

# REDIRECTION TO HTTPS
RewriteEngine On
RewriteCond %{HTTP:X-Forwarded-Port} 80
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R,L]

Options +FollowSymlinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^www.geogeoquidev.fr$
RewriteRule ^(.*) https://geogeoquidev.fr/$1 [QSA,L,R=301]

